{
  "name": "ts-lib-starter",
  "version": "0.0.1",
  "description": "Start Node.js / ECMAScript 6 module with TypeScript",
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=10.10.0"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:es6",
    "build:main": "rimraf lib/main && tsc -p tsconfig.build.json",
    "build:es6": "rimraf lib/module && ttsc -p tsconfig.build.es6.json",
    "lint": "eslint \"{src,test,examples,demo}/**/*.{ts,js}\" --fix",
    "test": "jest"
  },
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "13.1.4",
    "@types/rimraf": "2.0.3",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "@zoltu/typescript-transformer-append-js-extension": "1.0.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.2.0",
    "ts-node-dev": "1.0.0-pre.44",
    "ttypescript": "1.5.8",
    "typescript": "3.7.4"
  },
  "dependencies": {}
}
